<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.smart.home.domain">
	<class name="UserConcentrator" table="user_concentrator">

		<composite-id>
		   <key-property name="userID" column="USER_ID"  ></key-property>
		
	       <key-property name="concentratorID" column="CONCENTRATOR_ID"  ></key-property>
	    </composite-id>
	    
	     <property name="operate" column="OPERATE" not-null="true"></property>


	</class>
</hibernate-mapping>