<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.smart.home.domain">
	<class name="SensorType" table="sensor_type">

	    <id name="typeID" column="TYPE_ID" >
	       <generator class="identity"/> 
		</id>
		 <property name="typeName" column="TYPE_NAME"></property>
		 <property name="defWarnValue" column="DEF_WARN_VALUE"></property>	
		 <property name="defErrorValue" column="DEF_ERROR_VALUE"></property>
		 <property name="typeCode" column="TYPE_CODE"></property>	
		 <property name="typeSys" column="TYPE_SYS"></property>		

	</class>
 
</hibernate-mapping>