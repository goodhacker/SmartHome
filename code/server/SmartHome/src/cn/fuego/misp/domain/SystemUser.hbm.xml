<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.misp.domain">
	<class name="SystemUser" table="misp_user">
			<id name="userID" column="Ui_ID" >
			<generator class="increment"/>
			</id>
            <property name="userName" column="Us_Account"  not-null="true" ></property>
        
            <property name="password" column="Us_PassWord"  not-null="true" ></property>
            <property name="accountType" column="Ui_AccountType"  length='20' ></property>
            <property name="regDate" column="Ud_RegDate"  ></property>

	
				
	</class>
</hibernate-mapping>